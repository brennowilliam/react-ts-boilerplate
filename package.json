{
  "name": "ts-react",
  "version": "1.0.0",
  "description": "A simple skeleton to start a react app.",
  "main": "index.js",
  "scripts": {
    "webpack": "webpack",
    "webpack-dev-server": "webpack-dev-server",
    "prod": "npm run webpack -- --env.mode production",
