    "prod:debug": "npm run debug -- --env.mode production",
    "dev:debug": "npm run debug -- --env.mode debug",
    "lint": "tslint -c ./tslint.json './src/**/*.ts*'"
  },
  "keywords": [],
  "author": "",
  "license": "ISC",
  "dependencies": {
    "lodash": "^4.17.10",
    "moment": "^2.22.2",
